{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "next/core-web-vitals",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": [
      "error",
      { "vars": "all", "args": "none", "varsIgnorePattern": "^_" }
    ],
    "@typescript-eslint/no-empty-function": "off"
  }
}
